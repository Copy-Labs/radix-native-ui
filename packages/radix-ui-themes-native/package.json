{
  "name": "radix-native-ui",
  "version": "0.1.0-alpha.0",
  "description": "Radix Themes for React Native and Expo",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm exec tsc",
    "dev": "pnpm exec tsc --watch",
    "build:tsdown": "tsdown",
    "dev:tsdown": "tsdown --watch",
    "test": "jest",
    "yalc:publish": "yalc publish",
    "yalc:push": "yalc push",
    "yalc:watch": "yalc publish --watch",
    "dev:for-demo-app": "pnpm dev:tsdown && pnpm yalc:watch"
  },
  "peerDependencies": {
    "expo": ">=48",
    "react": ">=18",
    "react-native": ">=0.70"
  },
  "dependencies": {
    "react-native-modal": "^13.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.0",
    "@types/react": "^19.0.2",
    "jest": "^29.6.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-test-renderer": "^18.2.0",
    "tsdown": "^0.20.1",
    "typescript": "^5.1.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Copy-Labs/radix-native-ui.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "radix",
    "themes",
    "ui-components",
    "accessibility"
  ],
  "bugs": {
    "url": "https://github.com/Copy-Labs/radix-native-ui/issues"
  },
  "homepage": "https://github.com/Copy-Labs/radix-native-ui#readme"
}
